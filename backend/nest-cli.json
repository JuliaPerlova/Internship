{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-gateway/src",
  "monorepo": true,
  "root": "apps/api-gateway/src",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
  },
  "projects": {
    "user-service": {
      "type": "application",
      "root": "apps/user-service/src",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    },
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway/src",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "token-service": {
      "type": "application",
      "root": "apps/token-service/src",
      "entryFile": "main",
      "sourceRoot": "apps/token-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/token-service/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service/src",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "mail-service": {
      "type": "application",
      "root": "apps/shared/mail-service/src",
      "entryFile": "main",
      "sourceRoot": "apps/shared/mail-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/shared/mail-service/tsconfig.app.json"
      }
    },
    "database": {
      "type": "application",
      "root": "apps/shared/database/src",
      "entryFile": "main",
      "sourceRoot": "apps/shared/database/src",
      "compilerOptions": {
        "tsConfigPath": "apps/shared/database/tsconfig.app.json"
      }
    },
    "post-service": {
      "type": "application",
      "root": "apps/post-service/src",
      "entryFile": "main",
      "sourceRoot": "apps/post-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/post-service/tsconfig.app.json"
      }
    }
  }
}
